{
  "name": "vendoor-backend",
  "version": "1.0.50",
  "scripts": {
    "auth:gen": "bun run @better-auth/cli generate",
    "build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile server src/server.ts",
    "codegen": "graphql-codegen",
    "db:migrate": "bun run prisma db push",
    "dev": "bun run --watch src/server.ts",
    "start:prod": "./server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@elysiajs/apollo": "^1.4.0",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/openapi": "^1.4.11",
    "@graphql-tools/schema": "^10.0.25",
    "@graphql-tools/utils": "^10.9.1",
    "@prisma/client": "^6.17.1",
    "better-auth": "^1.3.27",
    "elysia": "latest",
    "envalid": "^8.1.0",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.25.0",
    "json-bigint-patch": "^0.0.8"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.27",
    "@eddeee888/gcg-typescript-resolver-files": "^0.14.0",
    "@graphql-codegen/cli": "^6.0.0",
    "bun-types": "latest",
    "prisma": "^6.17.1"
  },
  "module": "src/server.js"
}
